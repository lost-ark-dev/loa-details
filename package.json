{
  "name": "loa-details",
  "version": "1.4.2",
  "description": "Damage meter for Lost Ark",
  "productName": "LOA Details",
  "author": "Herysia, Mathicha, Eren Kara <erenkyi0@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./ && npm run typecheck",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "format": "prettier --write \"**/*.{js,ts,vue,css,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev -m electron",
    "build": "npm run lint && quasar build -m electron",
    "release": "npm run lint && quasar build -m electron -P always",
    "install": "electron-rebuild"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "dayjs": "^1.11.7",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "lodash": "^4.17.21",
    "meter-core": "github:lost-ark-dev/meter-core"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@quasar/app-vite": "^1.2.2",
    "@quasar/extras": "^1.16.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "autoprefixer": "^10.4.14",
    "electron": "^24.0.0",
    "electron-builder": "^22.14.13",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.11.0",
    "html2canvas": "^1.4.1",
    "pinia": "^2.0.35",
    "prettier": "^2.8.7",
    "quasar": "^2.11.10",
    "typescript": "^4.9.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.6.5"
  },
  "engines": {
    "node": "^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
